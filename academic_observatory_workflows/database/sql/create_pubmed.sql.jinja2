with PubmedTable as (
SELECT 
  UPPER(TRIM(ArticleIds.value)) as doi,
  *,
FROM 
  `{{ pubmed.project_id }}.{{ pubmed.dataset_id }}.{{ pubmed.table_id }}`,
  UNNEST(PubmedData.ArticleIdList) AS ArticleIds
WHERE 
  ArticleIds.IdType = 'doi'
  and
  ArticleIds.value is not NULL)

SELECT * EXCEPT (IdType, value)
FROM PubmedTable
